<% @makers.each {|maker| %>
    <div class="row">
        <div class="col-md-5">
            <%= maker['maker'] %>
        </div>
    </div>
    <% maker['models'][0].each { |model| %>
        <div class="row">
            <div class="col-md-3">
                <a href="/cars/<%= maker['maker'] %>/<%= model['model'] %>"><%= model['model'] %></a>
            </div>
            <div class="col-md-5">
                <%= model['model'] %>
            </div>
        </div>
    <% } %>
<% } %>

